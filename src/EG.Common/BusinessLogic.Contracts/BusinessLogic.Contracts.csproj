        <AssemblyName>EncyclopediaGalactica.BusinessLogic.Contracts</AssemblyName>
        <RootNamespace>EncyclopediaGalactica.BusinessLogic.Contracts</RootNamespace>
        <Title>EncyclopediaGalactica.BusinessLogic.Contracts</Title>
        <PackageId>EncyclopediaGalactica.BusinessLogic.Contracts</PackageId>
